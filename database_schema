--This is to make the tables to hold the weather data
CREATE TABLE london (
	years INT NOT NULL,
    months INT NOT NULL,
    tmax_degC INT,
    tmin_degC INT,
    rain_mm INT,
    tmax_degF INT,
    tmin_degF INT,
    monthly_total_precip INT
);

CREATE TABLE new_york (
	years INT NOT NULL,
    months INT NOT NULL,
    tmax_degC INT,
    tmin_degC INT,
    rain_mm INT,
    tmax_degF INT,
    tmin_degF INT,
    monthly_total_precip INT,
    monthly_total_snowfall INT
);


--This would be to build table with just what is wanted to be pulled

SELECT months, tmin_degC, af_days, rain_mm, sun_hours, tmin_degC
INTO london
FROM london_imported


SELECT months, tmin_degF, monthly_total_precip, monthly_total_snowfall, tmax_degF
INTO new_york
FROM new_york_imported
