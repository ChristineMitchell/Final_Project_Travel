select * from new_york;

select * from ny_extra;

SELECT new_york."Names",
    new_york."Geometry",
	new_york."Address",
	new_york."Lattitude",
	new_york."Longitude",
	new_york."Category",
    ny_extra."Details"
	INTO new_york_joined
	FROM new_york
	LEFT JOIN ny_extra
	ON new_york."Names" = ny_extra."Names";
	
Select * from new_york_joined;